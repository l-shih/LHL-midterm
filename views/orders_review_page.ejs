<% include ./partials/_header %>
<html>
  <head>
    <meta charset='utf-8'>
    <title>Review Your Order</title>
  </head>
  <body>
    <% console.log(results) %>
    <main id='order_main'>
      This is where you can review the details of your order.
      <br>
      <ul>
        <% for (var i = 0; i < results.length; i++) {%>
          <li><%= results[i].user_id %></li>
          <li><%= results[i].total_price %></li>
          <li><%= results[i].paid_at %></li>
        <% } %>
        <br/>
      </ul>
    </main>
  </body>
</html>
<% include ./partials/_footer %>